          },
          "editorMode": "code",
          "expr": "avg_over_time({kind=\"measurement\",app_name=\"$app\"} |= \" lcp=\" | logfmt | unwrap lcp [$__range]) by (app)",
          "legendFormat": "LCP",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "LCP",
      "type": "stat"
